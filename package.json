{
  "name": "cypress-ui-framework",
  "version": "1.0.0",
  "description": "cypress automation test",
  "main": "index.js",
  "scripts": {
    "run": "cypress run --spec=cypress/e2e/",
    "open": "cypress open",
    "watch": "cypress watch cypress/e2e/",
    "run:firefox": "cypress run --browser firefox --spec=cypress/e2e/",
    "run:chrome": "cypress run --browser chrome --spec=cypress/e2e/",
    "run:electron": "cypress run --browser electron --spec=cypress/e2e/",
    "run:webkit": "cypress run --browser webkit --spec=cypress/e2e/",
    "run:firefox:headless": "cypress run --browser firefox --spec=cypress/e2e/ --headless",
    "run:chrome:headless": "cypress run --browser chrome --spec=cypress/e2e/ --headless",
    "run:electron:headless": "cypress run --browser electron --spec=cypress/e2e/ --headless",
    "run:webkit:headless": "cypress run --browser webkit --spec=cypress/e2e/ --headless",
    "run:visual": "cypress run --browser chrome --env visual=true --spec=cypress/visual-regression/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^24.1.0",
    "cypress": "^15.0.0",
    "cypress-visual-regression": "^5.3.0",
    "typescript": "^5.8.3"
  }
}
