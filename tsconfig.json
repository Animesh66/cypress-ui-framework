{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["ES2015", "dom"],
    "types": ["cypress", "node"],
    "module": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
